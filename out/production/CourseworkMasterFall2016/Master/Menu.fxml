<!--"Scene design information for the Master Scene MasterBorderPane"-->

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane fx:id="MasterBorderPane" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" stylesheets="@MasterWindowStyle.css"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Master.MasterController">
    <top>
        <VBox fx:id="MasterTitleVBox" BorderPane.alignment="CENTER">
            <Label fx:id="MasterLabel" text="Master">
                <font>
                    <Font name="Gabriola" size="36.0"/>
                </font>
                <padding>
                    <Insets left="20.0"/>
                </padding>
            </Label>
            <Label id="ProjectChoiceLabel" fx:id="MasterProjectChoiceLabel" text="Chose A Project">
                <font>
                    <Font name="Gabriola" size="18.0"/>
                </font>
                <padding>
                    <Insets left="50.0"/>
                </padding>
            </Label>
            <MenuBar fx:id="MasterMenuBar">
                <Menu fx:id="MasterMenu285" mnemonicParsing="false" text="CSC285">
                    <Menu fx:id="HomeworkMenu285" mnemonicParsing="false" text="Homework">
                        <MenuItem id="HomeworkMenu1" fx:id="HomeworkMenu285Item1" mnemonicParsing="false"
                                  onAction="#Homework01Click" text="Homework_01"/>
                    </Menu>
                    <Menu fx:id="PracticekMenu285" mnemonicParsing="false" text="FXPractice">
                        <MenuItem id="PracticeMenu1" fx:id="PracticeMenu285Item1" mnemonicParsing="false"
                                  onAction="#Practice01Click" text="Practice_01"/>
                        <MenuItem id="PracticeMenu2" fx:id="PracticeMenu285Item2" mnemonicParsing="false"
                                  onAction="#Practice02Click" text="Practice_02"/>
                        <MenuItem id="PracticeMenu3" fx:id="PracticeMenu285Item3" mnemonicParsing="false"
                                  onAction="#Practice03Click" text="Practice_03"/>
                        <MenuItem id="PracticeMenu4" fx:id="PracticeMenu285Item4" mnemonicParsing="false"
                                  onAction="#Practice04Click" text="Practice_04"/>
                        <MenuItem id="PracticeMenu5" fx:id="PracticeMenu285Item5" mnemonicParsing="false"
                                  onAction="#Practice05Click" text="Practice_05"/>
                    </Menu>
                </Menu>
                <Menu fx:id="MasterMenu406" mnemonicParsing="false" text="CSC406">
                    <Menu fx:id="ThreadPracticeMenu406" mnemonicParsing="false" text="Thread Practice">
                        <MenuItem id="ThreadPracticeMenu01" fx:id="ThreadPractice406Item1"
                                  mnemonicParsing="false" onAction="#ThreadPractice01Click"
                                  text="ThreadPractice_01"/>
                        <MenuItem id="ThreadPracticeMenu02" fx:id="ThreadPractice406Item2"
                                  mnemonicParsing="false" onAction="#ThreadPractice02Click"
                                  text="ThreadPractice_02"/>
                    </Menu>
                </Menu>
            </MenuBar>
        </VBox>
    </top>
    <center>
        <StackPane id="MasterContentPane" fx:id="MasterContentPane" BorderPane.alignment="CENTER">
            <BorderPane.margin>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </BorderPane.margin>
        </StackPane>
    </center>
</BorderPane>
